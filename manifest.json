{
  "manifest_version": 2,
  "name": "cookies copy",
  "version": "1.0.0",
  "description": "Copy cookies of current address to another address",
  "icons": {
    "16": "images/icon.png",
    "48": "images/icon.png",
    "128": "images/icon.png"
  },
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_title": "Title",
    "default_popup": "popup.html"
  },
  "permissions": [
    "cookies",
    "tabs",
    "notifications",
    "*://*/*"
  ]
}